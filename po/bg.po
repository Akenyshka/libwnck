#: ../libwnck/wnckprop.c:176 ../libwnck/wnckprop.c:243
msgid "Y"
msgstr "Y"

#: ../libwnck/wnckprop.c:182
msgid "Minimize the window"
msgstr "Минимизиране на прозореца"

#: ../libwnck/wnckprop.c:184
msgid "Unminimize the window"
